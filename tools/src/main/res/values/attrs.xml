<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TVChatView">
        <attr name="idTitle" format="string"/>
        <attr name="idMessageList" format="string"/>
        <attr name="idInput" format="string"/>
    </declare-styleable>

    <declare-styleable name="WaterRippleView">
        <attr name="rippleSpacing" format="dimension|reference"/>
        <attr name="rippleCount" format="integer|reference"/>
        <attr name="rippleColor" format="color|reference"/>
        <attr name="rippleAutoRunning" format="boolean"/>
        <attr name="rippleExpand" format="boolean"/>
        <attr name="rippleMixRadius" format="integer"/>
        <attr name="rippleCenterX" format="integer"/>
        <attr name="rippleCenterY" format="integer"/>
    </declare-styleable>
</resources>